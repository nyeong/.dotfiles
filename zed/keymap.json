[
  {
    "context": "Editor",
    "bindings": {
      "alt-o": "editor::SelectLargerSyntaxNode",
      "alt-i": "editor::SelectSmallerSyntaxNode"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "space k": "editor::Hover",
      "space s": "outline::Toggle",
      "space S": "project_symbols::Toggle"
    }
  }
]
